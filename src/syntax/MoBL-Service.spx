package mobltemplatelang
module MoBL-Service
templates
  Definition.Service = <
    <MetaAnno*; separator="\n">
    service <QId> {
      <ServiceElem*; separator="\n">
    }
  >
  ServiceElem = <<PropVal>>
  ServiceElem.Resource = <
    <MetaAnno*; separator="\n">
    resource <ID>(<FArg*; separator=", ">) : <Type> {
      <ResourceElem*; separator="\n">
    }
  >
  ResourceElem = <<PropVal>>
